    <Form.Group controlId='email'>\n                    <Form.Label>Image</Form.Label>\n                    <Form.Control\n                        type='file'\n                        placeholder='Enter email'\n                        value={email}\n                        onChange={(e) => setEmail(e.target.value)}\n                    ></Form.Control>\n                </Form.Group>\n\n                <Form.Group controlId='phoneNumber'>\n                    <Form.Label>Discription</Form.Label>\n                    <Form.Control\n                        type='text'\n                        placeholder='Enter phone number'\n                        value={phoneNumber}\n                        onChange={(e) => setPhoneNumber(e.target.value)}\n                    ></Form.Control>\n                </Form.Group>\n\n                <Form.Group controlId='password'>\n                    <Form.Label>Brand</Form.Label>\n                    <Form.Control\n                        type='password'\n                        placeholder='Enter password'\n                        value={password}\n                        onChange={(e) => setPassword(e.target.value)}\n                    ></Form.Control>\n                </Form.Group>\n\n                <Form.Group controlId='confirmPassword'>\n                    <Form.Label>Category</Form.Label>\n                    <Form.Control\n                        type='password'\n                        placeholder='Enter confirm password'\n                        value={confirmPassword}\n                        onChange={(e) => setConfirmPassword(e.target.value)}\n                    ></Form.Control>\n                </Form.Group>\n\n                <Form.Group controlId='confirmPassword'>\n                    <Form.Label>Price</Form.Label>\n                    <Form.Control\n                        type='password'\n                        placeholder='Enter confirm password'\n                        value={confirmPassword}\n                        onChange={(e) => setConfirmPassword(e.target.value)}\n                    ></Form.Control>\n                </Form.Group>\n\n                <Form.Group controlId='confirmPassword'>\n                    <Form.Label>Category</Form.Label>\n                    <Form.Control\n                        type='password'\n                        placeholder='Enter confirm password'\n                        value={confirmPassword}\n                        onChange={(e) => setConfirmPassword(e.target.value)}\n                    ></Form.Control>\n                </Form.Group>\n\n                <Button type='submit' variant='primary'>Add</Button>\n            </Form>\n        </FormContainer>\n    );\n}\n\nexport default AddItem;","/home/amrit/Desktop/WebD/Sero/client/src/Screens/UpdateDetails.js",["194","195","196"],{"ruleId":"197","replacedBy":"198"},{"ruleId":"199","replacedBy":"200"},{"ruleId":"201","severity":1,"message":"202","line":2,"column":8,"nodeType":"203","messageId":"204","endLine":2,"endColumn":16},{"ruleId":"201","severity":1,"message":"205","line":2,"column":39,"nodeType":"203","messageId":"204","endLine":2,"endColumn":42},{"ruleId":"201","severity":1,"message":"206","line":2,"column":44,"nodeType":"203","messageId":"204","endLine":2,"endColumn":50},{"ruleId":"201","severity":1,"message":"207","line":1,"column":17,"nodeType":"203","messageId":"204","endLine":1,"endColumn":26},{"ruleId":"201","severity":1,"message":"208","line":1,"column":17,"nodeType":"203","messageId":"204","endLine":1,"endColumn":25},{"ruleId":"201","severity":1,"message":"209","line":2,"column":10,"nodeType":"203","messageId":"204","endLine":2,"endColumn":14},{"ruleId":"201","severity":1,"message":"210","line":2,"column":16,"nodeType":"203","messageId":"204","endLine":2,"endColumn":26},{"ruleId":"201","severity":1,"message":"211","line":4,"column":10,"nodeType":"203","messageId":"204","endLine":4,"endColumn":13},{"ruleId":"201","severity":1,"message":"212","line":4,"column":15,"nodeT